{
  "name": "call-graph",
  "displayName": "call-graph",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "call-graph.helloWorld",
        "title": "Hello World",
        "icon": {
          "dark": "assets/ui-hint.svg",
          "light": "assets/ui-hint.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "call-graph.helloWorld",
          "alt": "Hello World",
          "when": "inDebugMode",
          "group": "navigation"
        }
      ]
    }
  },
  "files": [
    "frontend/dist",
    "frontend/dist/assets"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile-frontend && npm run package",
    "compile": "npm run compile-frontend && webpack",
    "watch": "npm run compile-frontend && webpack --watch",
    "package": "npm run compile-frontend && webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "compile-frontend": "cd frontend && npm run build"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "ts-loader": "^9.5.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}